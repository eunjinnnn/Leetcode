def isPalindrome(self, x: int) -> bool:
        x=str(x)
        i=0
        while i<len(x)-1-i:
            if x[i]==x[len(x)-1-i]:
                i+=1
                continue
            else:
                return False
        return True
            
def isPalindrome(self, x: int) -> bool:
        x=str(x)
        if x==x[::-1]:
          return True
